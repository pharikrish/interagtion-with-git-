def GIT_CREDENTIALS = "gitcredentials"

pipeline{
agent any
        stages {
             stage('check-out-code'){
                 steps{ 
                        git credentialsId: GIT_CREDENTIALS,
                        url:'https://github.com/pharikrish/Dev-Ops-TEST.git', branch:'main'
                    }
                }
                stage('build'){
                    steps{
                        echo 'build'
                    }
                }
                stage('test'){
                    steps{
                        echo 'unit testing'
                    }
                }
                stage('artifact'){
                    steps{
                        echo 'artifacts'
                    }
                }
                stage('snyk'){
                    steps{
                        echo 'snyk test'
                    }
                }
                stage('Deploy'){
                    steps{
                        echo 'Deply stage'
                    }
                }
             
           }
    }