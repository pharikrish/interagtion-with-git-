def GIT_CREDENTIALS= "dev-token"

    pipeline{
    agent
         stage{
             stage('check-out-code'){
                 steps{
                   script{
                    git credentialsId: GIT_CREDENTIALS,
                    url:"https://github.com/pharikrish/Dev-Ops-TEST.git"

                }
            }
        }
    }
    
}